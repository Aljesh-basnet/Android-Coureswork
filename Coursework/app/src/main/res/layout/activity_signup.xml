<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2F394D"
    tools:context=".Signup">

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="183dp"
        android:layout_height="174dp"
        android:layout_marginLeft="110dp"
        android:layout_marginTop="50dp"
        app:srcCompat="@drawable/logo" />

    <EditText
        android:id="@+id/signup_username"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView11"
        android:layout_alignStart="@+id/imageView11"
        android:layout_alignEnd="@+id/imageView11"
        android:layout_alignBottom="@+id/imageView11"
        android:layout_marginStart="-90dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="-84dp"
        android:layout_marginBottom="-81dp"
        android:ems="10"
        android:fontFamily="@font/chelsea_market"
        android:hint="     Username"
        android:inputType="textPersonName"
        android:textColor="#EEE1B3"
        android:textColorHint="#EEE1B3"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/signup_email"
        android:layout_width="297dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/signup_username"
        android:layout_alignTop="@+id/signup_username"
        android:layout_alignEnd="@+id/signup_username"
        android:layout_alignBottom="@+id/signup_username"
        android:layout_marginStart="2dp"
        android:layout_marginTop="62dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="-61dp"
        android:ems="10"
        android:fontFamily="@font/chelsea_market"
        android:hint="     Email   "
        android:inputType="textPersonName"
        android:textColor="#EEE1B3"
        android:textColorHint="#EEE1B3" />

    <EditText
        android:id="@+id/signup_password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignStart="@+id/signup_email"
        android:layout_alignTop="@+id/signup_email"
        android:layout_alignEnd="@+id/signup_email"
        android:layout_alignBottom="@+id/signup_email"
        android:layout_marginStart="1dp"
        android:layout_marginTop="65dp"
        android:layout_marginEnd="-1dp"
        android:layout_marginBottom="-68dp"
        android:ems="10"
        android:fontFamily="@font/chelsea_market"
        android:hint="     Password"
        android:inputType="textPersonName"
        android:textColor="#EEE1B3"
        android:textColorHint="#EEE1B3" />

    <EditText
        android:id="@+id/signup_confirmPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignStart="@+id/signup_password"
        android:layout_alignTop="@+id/password"
        android:layout_alignEnd="@+id/signup_password"
        android:layout_alignBottom="@+id/signup_password"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="65dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="-68dp"
        android:ems="10"
        android:fontFamily="@font/chelsea_market"
        android:hint="     Confirm Password"
        android:inputType="textPersonName"
        android:password="false"
        android:textColor="#EEE1B3"
        android:textColorHint="#EEE1B3"
        android:textSize="18sp" />

    <ImageView
        android:id="@+id/username_icon"
        android:layout_width="27dp"
        android:layout_height="wrap_content"


        android:layout_alignStart="@+id/signup_username"
        android:layout_alignTop="@+id/signup_username"
        android:layout_alignEnd="@+id/signup_username"
        android:layout_alignBottom="@+id/signup_username"
        android:layout_marginStart="-19dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="348dp"
        android:layout_marginBottom="10dp"
        app:srcCompat="@drawable/ic_person_24px" />

    <ImageView
        android:id="@+id/confirm_password_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/signup_password"
        android:layout_alignTop="@+id/password"
        android:layout_alignEnd="@+id/password"
        android:layout_alignBottom="@+id/signup_password"
        android:layout_marginStart="-13dp"
        android:layout_marginTop="77dp"
        android:layout_marginEnd="192dp"
        android:layout_marginBottom="10dp"
        app:srcCompat="@drawable/ic_no_encryption_24px" />

    <ImageView
        android:id="@+id/password_icon"
        android:layout_width="23dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/signup_confirmPassword"
        android:layout_alignTop="@+id/password"
        android:layout_alignEnd="@+id/signup_confirmPassword"
        android:layout_alignBottom="@+id/signup_confirmPassword"
        android:layout_marginStart="-15dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="346dp"
        android:layout_marginBottom="10dp"
        app:srcCompat="@drawable/ic_no_encryption_24px" />

    <ImageView
        android:id="@+id/email_icon"
        android:layout_width="33dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/signup_email"
        android:layout_alignTop="@+id/signup_email"
        android:layout_alignEnd="@+id/signup_email"
        android:layout_alignBottom="@+id/password"
        android:layout_marginStart="-22dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="344dp"
        android:layout_marginBottom="77dp"
        app:srcCompat="@drawable/ic_mail_outline_24px" />

    <Button
        android:id="@+id/signup_createAccount"
        android:layout_width="wrap_content"
        android:layout_height="38dp"
        android:layout_alignStart="@+id/imageView11"
        android:layout_alignTop="@+id/imageView11"
        android:layout_alignEnd="@+id/imageView11"
        android:layout_alignBottom="@+id/imageView11"
        android:layout_marginStart="-2dp"
        android:layout_marginTop="466dp"
        android:layout_marginEnd="-15dp"
        android:layout_marginBottom="-334dp"
        android:background="@drawable/button_default"
        android:fontFamily="@font/chelsea_market"
        android:text="Create Account"
        android:textSize="12sp" />

    <Button
        android:id="@+id/signup_login"
        android:layout_width="129dp"
        android:layout_height="39dp"
        android:layout_alignStart="@+id/imageView11"
        android:layout_alignTop="@+id/imageView11"
        android:layout_alignEnd="@+id/imageView11"
        android:layout_alignBottom="@+id/imageView11"
        android:layout_marginStart="6dp"
        android:layout_marginTop="538dp"
        android:layout_marginEnd="-7dp"
        android:layout_marginBottom="-407dp"
        android:background="@drawable/button_uncolored"
        android:fontFamily="@font/chelsea_market"
        android:text="Login"
        android:textSize="12sp" />


</RelativeLayout>