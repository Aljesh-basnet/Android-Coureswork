<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2F394D"
    tools:context=".Signup">
    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="126dp"
        android:layout_height="197dp"
        android:layout_marginLeft="154dp"
        android:layout_marginTop="80dp"
        app:srcCompat="@drawable/Logo" />

    <EditText
        android:id="@+id/email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView11"
        android:layout_alignStart="@+id/imageView11"
        android:layout_alignEnd="@+id/imageView11"
        android:layout_alignBottom="@+id/imageView11"
        android:layout_marginStart="-33dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="-47dp"
        android:layout_marginBottom="-65dp"
        android:ems="10"
        android:hint="     Username"
        android:inputType="textPersonName"
        android:textColor="#EEE1B3"
        android:textColorHint="#EEE1B3" />

    <EditText
        android:id="@+id/username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView11"
        android:layout_alignTop="@+id/email"
        android:layout_alignEnd="@+id/email"
        android:layout_alignBottom="@+id/email"
        android:layout_marginStart="-31dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="-62dp"
        android:ems="10"
        android:hint="     Email"
        android:inputType="textPersonName"
        android:textColor="#EEE1B3"
        android:textColorHint="#EEE1B3" />

    <EditText
        android:id="@+id/password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignStart="@+id/username"
        android:layout_alignTop="@+id/username"
        android:layout_alignEnd="@+id/username"
        android:layout_marginStart="0dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="-2dp"
        android:layout_marginBottom="-226dp"
        android:ems="10"
        android:hint="    Password"
        android:inputType="textPersonName"
        android:textColor="#EEE1B3"
        android:textColorHint="#EEE1B3" />

    <EditText
        android:id="@+id/confirmPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignStart="@+id/password"
        android:layout_alignTop="@+id/password"
        android:layout_alignEnd="@+id/password"
        android:layout_alignBottom="@+id/password"
        android:layout_marginStart="0dp"
        android:layout_marginTop="65dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="-65dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:password="false"
        android:hint="    Confirm Password"
        android:textColor="#EEE1B3"
        android:textColorHint="#EEE1B3" />

    <ImageView
        android:id="@+id/username_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/username"
        android:layout_alignTop="@+id/username"
        android:layout_alignEnd="@+id/username"
        android:layout_alignBottom="@+id/username"
        android:layout_marginStart="4dp"
        android:layout_marginTop="-50dp"
        android:layout_marginEnd="189dp"
        android:layout_marginBottom="79dp"
        app:srcCompat="@drawable/ic_person_24px" />

    <ImageView
        android:id="@+id/confirm_password_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/password"
        android:layout_alignTop="@+id/password"
        android:layout_alignEnd="@+id/password"
        android:layout_alignBottom="@+id/password"
        android:layout_marginStart="4dp"
        android:layout_marginTop="77dp"
        android:layout_marginEnd="192dp"
        android:layout_marginBottom="-51dp"
        app:srcCompat="@drawable/ic_no_encryption_24px" />

    <ImageView
        android:id="@+id/password_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/password"
        android:layout_alignTop="@+id/password"
        android:layout_alignEnd="@+id/password"
        android:layout_alignBottom="@+id/password"
        android:layout_marginStart="4dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="192dp"
        android:layout_marginBottom="15dp"
        app:srcCompat="@drawable/ic_no_encryption_24px" />

    <ImageView
        android:id="@+id/email_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/password"
        android:layout_alignTop="@+id/password"
        android:layout_alignEnd="@+id/password"
        android:layout_alignBottom="@+id/password"
        android:layout_marginStart="3dp"
        android:layout_marginTop="-51dp"
        android:layout_marginEnd="189dp"
        android:layout_marginBottom="77dp"
        app:srcCompat="@drawable/ic_mail_outline_24px" />

    <Button
        android:id="@+id/createAccount"
        android:layout_width="137dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView11"
        android:layout_alignTop="@+id/imageView11"
        android:layout_alignEnd="@+id/imageView11"
        android:layout_alignBottom="@+id/imageView11"
        android:layout_marginStart="0dp"
        android:layout_marginTop="468dp"
        android:layout_marginEnd="-11dp"
        android:layout_marginBottom="-324dp"
        android:background="@drawable/button_default"
        android:text="Create Account" />

    <Button
        android:id="@+id/login"
        android:layout_width="113dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView11"
        android:layout_alignTop="@+id/imageView11"
        android:layout_alignEnd="@+id/imageView11"
        android:layout_alignBottom="@+id/imageView11"
        android:layout_marginStart="13dp"
        android:layout_marginTop="540dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="-393dp"
        android:background="@drawable/button_uncolored"
        android:text="Login" />


</RelativeLayout>